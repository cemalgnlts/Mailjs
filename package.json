    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.1.4",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
